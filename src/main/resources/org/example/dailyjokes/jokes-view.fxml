<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.*?>
<AnchorPane prefHeight="390.0" prefWidth="365.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="org.example.dailyjokes.JokesController">
    <padding>
        <Insets left="15.0" right="15.0" top="50.0"/>
    </padding>
    <Button fx:id="btn" onAction="#onButtonClick" layoutX="125.0" layoutY="156.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="99.0" text="Click Me!">
        <font>
            <Font name="Bookman Old Style" size="15.0"/>
        </font>
    </Button>
    <TextField fx:id="username" layoutX="174.0" layoutY="41.0" prefWidth="163.0">
        <font>
            <Font name="Bookman Old Style" size="14.0"/>
        </font>
    </TextField>
    <Label layoutX="20.0" layoutY="45.0" prefHeight="26.0" prefWidth="120.0" text="Type your name">
        <font>
            <Font name="Bookman Old Style" size="14.0"/>
        </font>
    </Label>
    <Slider fx:id="slider" layoutX="174.0" layoutY="91.0" prefHeight="26.0" prefWidth="163.0" />
    <Label fx:id="sliderText" layoutX="28.0" layoutY="89.0" prefHeight="26.0" prefWidth="120.0" text="How's it going?">
        <font>
            <Font name="Bookman Old Style" size="14.0"/>
        </font>
    </Label>
    <TextArea fx:id="joke" editable="false" layoutX="40.0" layoutY="220.0" prefHeight="120.0" prefWidth="285.0"
              text="some prediction" visible="false" wrapText="true" >
        <font>
            <Font name="Bookman Old Style" size="15.0"/>
        </font>
    </TextArea>
</AnchorPane>
